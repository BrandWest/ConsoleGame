package appliction;

public class Fish extends Animal
{
	public Fish()
	{
		
	}
	public Fish( int age, int legs, String name )
	{
		_age = age;
		_legs = legs;
		_name = name;
	}
	
	@Override
	public void move()
	{
		_moveSpeed = 2.0;
		System.out.println( "I am a fish." );
	}
	
	@Override
	public String toString()
	{
		return "Legs: " + _legs + "\n" + "UserName: " + _name + "\n" + "Age: " + _age + "\n" + 
				"Movement Speed: " + _moveSpeed;
	}
}
