package appliction;

public class Animal 
{
	public double _moveSpeed;
	protected int _legs;
	protected int _age;
	public String _name;

	public Animal() 
	{
		
	}
	
	public Animal( int age, int legs, String name )
	{
		_age = 0;
		_legs = 2;
		_name = "animal";
	}
	
	protected void move()
	{
		_moveSpeed = 1.0;
		System.out.println( "I am an Animal." );
	}
	@Override
	public String toString()
	{
		return "Name: " + _name + "\n" + "UserName: " + _userName + "\n" + "Age: " + _age + "\n" + 
				"Address: " + _address;
	}	
}
