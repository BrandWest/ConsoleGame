package appliction;

public class Fish extends Animal
{
	public Fish()
	{
		
	}
	
	public Fish( int age, int legs, String name )
	{
		super( age, legs, name );
		_moveSpeed = movement;
	}
	
	@Override
	public void move()
	{
		_moveSpeed = 2.0;
		System.out.println( "I am a fish." );
	}
	
	@Override
	public String toString()
	{
		return super.toString() + "Movement Speed: " + _moveSpeed;
	}
}
