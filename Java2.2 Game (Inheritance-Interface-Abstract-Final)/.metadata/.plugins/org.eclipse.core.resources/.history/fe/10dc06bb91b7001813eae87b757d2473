package application;

public class Person
{
	public String name = "";
	protected String userName = "";
	int age = 0;
	private String address = "";
	
	public Person() {
		
	}
	public Person(String string, String string2, int i, String string3) {
		// TODO Auto-generated constructor stub
	}

	Person person = new Person( "Jim", "Jim123", 20, "123 Fake Road" );


	public void move()
	{
		System.out.println( "Move Person!");
	}
	
	@Override
	public String toString()
	{
		return "Name: " + name + "\n" + "UserName: " + userName + "\n" + "Age: " + age + "\n" + 
				"Address: " + address;
	}
	
	
	
}
