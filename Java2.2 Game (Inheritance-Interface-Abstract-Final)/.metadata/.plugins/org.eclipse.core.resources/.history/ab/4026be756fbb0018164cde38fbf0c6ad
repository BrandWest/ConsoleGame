package application;

public class TestSpaceObject 
{
	public static void main (String[] args )
	{
		SpaceObject [] spaceObj = new SpaceObject [ 2 ];
		Moon moon = new Moon( 4.4, 5.5, 0);
		Point point = new Point( 5.5, 5, 3);
		
		spaceObj [ 0 ] = moon;
		spaceObj [ 1 ] = point;
		
		moon.announce();
		point.announce();
	}

}
