package application;

public class TestSpaceObject 
{
	public static void main (String[] args )
	{
		SpaceObject [] spaceObj = new SpaceObject [ 2 ];
		
		spaceObj [ 0 ] = new Moon();
		spaceObj [ 2 ] = new Point();
		
		for ( int index = 0; index < spaceObj.length; index ++ )
		{
			System.out.println( spaceObj.announce() );
		}
	}

}
