package appliction;

public class Fish extends Animal
{
	public Fish()
	{
		
	}
	
	public Fish( int age, int legs, String name )
	{
		super( age, legs, name );
	}
	
	@Override
	public String move()
	{
		_moveSpeed = 2.0;
		return  "I am a fish.";
	}
	
	@Override
	public String toString()
	{
		return super.toString() + "Movement Speed: " + move() + _moveSpeed;
	}
}
