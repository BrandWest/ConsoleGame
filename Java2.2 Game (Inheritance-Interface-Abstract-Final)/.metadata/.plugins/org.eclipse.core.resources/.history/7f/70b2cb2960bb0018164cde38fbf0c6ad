package appliction;

public class Animal 
{
	public double _moveSpeed;
	protected int _legs;
	protected int _age;
	public String name;
	
	public static void main ( String[] args )
	{
		Fish fish = new Fish();
		Frog frog = new Frog();
		Bird bird = new Bird();
		Animal [] animal = new Animal [10];
		animal [ 1 ] = fish;
		for ( int index = 0; index < animal.length; index++ )
		{
			
		}
	}
	public Animal()
	{
		_age = 0;
		_legs = 2;
		_name = "animal";
	}
	protected void move()
	{
		_moveSpeed = 1.0;
	}
}
