package application;

public class Person
{
	public String name;
	protected String userName;
	int age;
	private String address;
	
	public Person() {
		
	}
	public Person(String string, String string2, int i, String string3) 
	{
		String name = string;
		String userName = string2;
		int age = i;
		String address = string3;
	}


	public void move()
	{
		System.out.println( "Move Person!");
	}
	
	@Override
	public String toString()
	{
		return "Name: " + name + "\n" + "UserName: " + userName + "\n" + "Age: " + age + "\n" + 
				"Address: " + address;
	}
	
	
	
}
