package application;

public class TestSpaceObject 
{
	public static void main (String[] args )
	{
		SpaceObject [] spaceObj = new SpaceObject [ 2 ];
		Moon moon = new Moon ();
		Point point = new Point();
		
		spaceObj [ 0 ] = new Moon();
		spaceObj [ 1 ] = new Point();
		
		moon.announce();
		point.announce();
	}

}
