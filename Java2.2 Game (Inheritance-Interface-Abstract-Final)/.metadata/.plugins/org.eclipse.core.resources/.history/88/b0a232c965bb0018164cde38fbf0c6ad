package appliction;

public class Bird extends Animal
{
	public Bird()
	{
		
	}
	
	public Bird( int age, int legs, String name )
	{
		super( age, legs, name );
	}
	
	
	@Override
	public String move()
	{
		_moveSpeed = 15.0;
		return  "I am a bird.";
	}
	
	@Override
	public String toString()
	{
		return super.toString() + "Movement Speed: " + move() + _moveSpeed;
	}
}