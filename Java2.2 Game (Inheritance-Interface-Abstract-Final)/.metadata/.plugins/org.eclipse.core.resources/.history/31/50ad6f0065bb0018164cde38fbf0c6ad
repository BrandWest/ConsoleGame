package appliction;

public class Dog extends Animal
{
	public Dog ()
	{
		
	}
	public Dog( int age, int legs, String name )
	{
		_age = age;
		_legs = legs;
		_name = name;
	}
	
	@Override
	public String move()
	{
		_moveSpeed = 2.0;
		return "I am a fish." ;
	}
	@Override
	public String toString()
	{
		return "Legs: " + _legs + "\n" + "UserName: " + _name + "\n" + "Age: " + _age + "\n" + 
				"Movement Speed: " + move();
	}
}
